{
    "name": "expressive-analytics/deep-thought",
    "type": "library",
    "description": "A highly versatile framework for attaching advanced object functionality to both legacy and modern platforms.",
    "keywords": ["ducktape","framework"],
    "homepage": "https://github.com/expressive-analytics/deep-thought.php",
    "license": "MIT",
    "authors": [
        {
            "name": "Blake Anderson",
            "email": "blake@expressiveanalytics.com",
            "homepage": "http://www.expressiveanalytics.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
	    "phpunit/PHPUnit":">=4.0.0"
    },
    "autoload": {
	    "psr-4":{
	    	"ExpressiveAnalytics\\DeepThought\\":
	    		["modules/utils",
	    		"modules/store",
	    		"modules/store-database",
	    		"modules/store-sqlite",
	    		"modules/store-mysql",
	    		"modules/store-pgsql",
	    		"modules/test",
	    		"modules/core"]
	    }
    },
    "autoload": {
	    "psr-4":{
	    	"ExpressiveAnalytics\\DeepThought\\":
	    		["modules/utils",
	    		"modules/store",
	    		"modules/store-database",
	    		"modules/store-sqlite",
	    		"modules/store-mysql",
	    		"modules/store-pgsql",
	    		"modules/test",
	    		"modules/core"],
		    "ExpressiveAnalytics\\DeepThought\\Tests\\": "tests/"
	    }
    }
}